-- Insert default availability for all doctors (Monday-Friday, 9 AM - 5 PM, 30-minute slots)
-- This allows appointments to be booked even if admin hasn't configured availability yet
INSERT INTO doctor_availabilities (id, doctor_id, day_of_week, start_time, end_time, slot_duration, is_active, created_at, updated_at)
SELECT 
  uuid_generate_v4(),
  d.id,
  1, -- Monday
  '09:00:00'::time,
  '17:00:00'::time,
  30,
  true,
  NOW(),
  NOW()
FROM doctors d
WHERE NOT EXISTS (
  SELECT 1 FROM doctor_availabilities da 
  WHERE da.doctor_id = d.id AND da.day_of_week = 1
)
UNION ALL
SELECT 
  uuid_generate_v4(),
  d.id,
  2, -- Tuesday
  '09:00:00'::time,
  '17:00:00'::time,
  30,
